<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Init</servlet-name>
        <servlet-class>Setup.initController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>Auth.AdminLoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Auth.LoginControlller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Auth.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminDashBoard</servlet-name>
        <servlet-class>Admin.DashboardController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Book</servlet-name>
        <servlet-class>Book.BookController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookDetail</servlet-name>
        <servlet-class>Book.BookDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Chapter</servlet-name>
        <servlet-class>Book.ChapterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBook</servlet-name>
        <servlet-class>Book.AddController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Category</servlet-name>
        <servlet-class>Book.CategoryController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCategory</servlet-name>
        <servlet-class>Book.EditCategoryController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>User.UserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddUser</servlet-name>
        <servlet-class>User.AddController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInfo</servlet-name>
        <servlet-class>User.InfoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rental</servlet-name>
        <servlet-class>Services.RentalController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentalConfirm</servlet-name>
        <servlet-class>Services.RentalConfirmController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentalList</servlet-name>
        <servlet-class>Services.RentalListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentalDetail</servlet-name>
        <servlet-class>Services.RentalDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Return</servlet-name>
        <servlet-class>Services.ReturnController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReturnList</servlet-name>
        <servlet-class>Services.ReturnListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReturnConfirmController</servlet-name>
        <servlet-class>Services.ReturnConfirmController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Init</servlet-name>
        <url-pattern>/Setup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/Admin-Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminDashBoard</servlet-name>
        <url-pattern>/Admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Book</servlet-name>
        <url-pattern>/Admin/Book</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookDetail</servlet-name>
        <url-pattern>/Admin/Book/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Chapter</servlet-name>
        <url-pattern>/Admin/Book/Chapter/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBook</servlet-name>
        <url-pattern>/Admin/Book/Add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Category</servlet-name>
        <url-pattern>/Admin/Book/Category</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCategory</servlet-name>
        <url-pattern>/Admin/Book/Category/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/Admin/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddUser</servlet-name>
        <url-pattern>/Admin/User/Add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInfo</servlet-name>
        <url-pattern>/Admin/User/Info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rental</servlet-name>
        <url-pattern>/Admin/Rental</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentalConfirm</servlet-name>
        <url-pattern>/Admin/Rental/Confirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentalList</servlet-name>
        <url-pattern>/Admin/Rental/List</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentalDetail</servlet-name>
        <url-pattern>/Admin/Rental/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Return</servlet-name>
        <url-pattern>/Admin/Return</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReturnList</servlet-name>
        <url-pattern>/Admin/Return/List</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReturnConfirmController</servlet-name>
        <url-pattern>/Admin/Return/Confirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>
